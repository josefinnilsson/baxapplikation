              <th>Drifter</th>
              <th>Enheter</th>
          </tr>
      </thead>
      <tbody>
        <% @alkohol.each do |u| %>
        <tr>
          <td><%= gravatar_for u, size: 250 %><%= u.name %></td>
          <td><%= u.alcohol %></td>
      </tr>
      <% end %>
  </tbody>
</table>
</div>
