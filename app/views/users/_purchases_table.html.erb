 <table class="purchases">
    <thead>
      <tr>
        <th>Produkt</th>
        <th>Pris</th>
        <th>Antal</th>
        <th>Summa</th>
      </tr>
    </thead>
    <tbody>
      <% @purchases_grouped.each do |product, purchases| %>
        <tr>
          <td><%= product.name %></td>
          <td><%= product.price %> kr</td>
          <td><%= purchases.size %></td>
          <td><%= product.price * purchases.size %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
